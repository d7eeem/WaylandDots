{
  "extractor": {
    "base-directory": "~/Downloads/gallery-dl",
    "parent-directory": false,
    "postprocessors": null,
    "archive": null,
    "cookies": null,
    "cookies-update": true,
    "proxy": "http://10.10.10.9:8888",
    "skip": true,
    "user-agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:128.0) Gecko/20100101 Firefox/128.0",
    "retries": 4,
    "timeout": 30,
    "verify": true,
    "fallback": true,
    "sleep": 0,
    "sleep-request": 5,
    "sleep-extractor": 5,
    "path-restrict": "auto",
    "path-replace": "_",
    "path-remove": "\\u0000-\\u001f\\u007f",
    "path-strip": "auto",
    "path-extended": true,
    "extension-map": {
      "jpeg": "jpg",
      "jpe": "jpg",
      "jfif": "jpg",
      "jif": "jpg",
      "jfi": "jpg"
    },
    "reddit": {
      "client-id": "redacted",
      "user-agent": "Python:glld:v1.0 (by /u/redacted)",
      "refresh-token": "cache",
      "comments": 0,
      "morecomments": false,
      "date-min": 0,
      "date-max": 253402210800,
      "date-format": "%Y-%m-%dT%H:%M:%S",
      "id-min": null,
      "id-max": null,
      "recursion": 0,
      "videos": true,
      "parent-directory": true,
      "directory": [
        "Reddit",
        "{subreddit}"
      ],
      "filename": "{date:%Y-%m-%d_%H-%M-%S}~_~{subreddit}~_~{id}-{num}.{author}~_~{title[0:27]}~_~{filename}.{extension}",
      "user": {
        "directory": [
          "Reddit",
          "{author}"
        ]
      }
    },
    "reddit>redgifs": {
      "directory": [
        "redgifs"
      ]
    },
    "redgifs": {
      "format": [
        "hd",
        "sd",
        "gif"
      ]
    },
    "twitter": {
      "cookies-from-browser": "firefox",
      "sleep": 5,
      "sleep-request": "6-12",
      "directory": [
        "Twitter",
        "{author}"
      ],
      "filename": "{tweet_id}_{num}.{extension}"
    },
    "instagram": {
      "cookies-from-browser": "chrome",
      "sleep": 5,
      "sleep-request": "6-12",
      "directory": [
        "Instagram",
        "{author}"
      ],
      "filename": "{shortcode}_{num}.{extension}"
    },
    "deviantart": {
      "client-id": "redacted",
      "client-secret": "redacted"
    },
    "bunkr": {
      "endpoint": "/api/_001_v2"
    }
  }
}
