{
  "wlr/taskbar": {
    "format": " {icon}",
    "icon-size": 14,
    "icon-theme": "MacTahoe-dark",
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-middle": "close",
    "ignore-list": [
      "Alacritty",
      "kitty",
      "steam",
      "discord"
    ],
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition"
    },
    "rewrite": {
      "Mozilla Firefox": "Firefox"
    }
  }
}
